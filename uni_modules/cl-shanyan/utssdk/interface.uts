// 自定义控件类型
export type WidgetConfig = {
	widgetId : string
	type : string
	left ?: string
	top ?: string
	right ?: string
	bottom ?: string
	width ?: string
	height ?: string
	textContent ?: string
	textFont ?: string
	textColor ?: string
	backgroundColor ?: string
	backgroundImgPath ?: string
	isFinish : string
}

// 所有控件集合类型
export type WidgetsConfig = Map<string, WidgetConfig>;

// 主配置类型
export type ShanYanUIConfigure = {
	//授权页固定元素配置
	uiConfig ?: AuthUIConfig
	//授权页自定义控件配置
	widgets ?: WidgetsConfig
}