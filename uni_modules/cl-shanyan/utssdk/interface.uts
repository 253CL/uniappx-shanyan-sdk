/**
 * 闪验SDK初始化、预取号、拉起授权页、获取token等统一返回结果对象封装
 */
export type CLResultResponse = {
	//外层状态码，1000：成功；其他：失败。
	code ?: number
	//外层描述
	message ?: string
	//内层状态码
	innerCode ?: number
	//内层描述
	innerDesc ?: string
	//其他信息
	data ?: ResultData
};
/**
 * 回调data数据封装
 */
export type ResultData = {
	number ?: string
	telecom ?: string
	protocolName ?: string
	protocolUrl ?: string
	token ?: string
}

/**
 * 一键登录初始化、预取号、拉起授权页、获取token等回调函数类型
 */
export type CLResultCallBack = (response : CLResultResponse) => void;


// 隐私协议项类型
export type PrivacyItem = {
	title : string
	url : string
}

// 隐私文本类型
export type PrivacyText = {
	privacyTextOne : string
	privacyTextTwo : string
	privacyTextThree : string
	privacyTextFour : string
	privacyTextFive : string
}

